{
    "name": "bytenet/laravel-admin-base",
    "type": "library",
    "description": "ByteNET Laravel Admin's base package, which offers admin authentication",
    "keywords": [
        "bytenet",
		"laravel",
		"admin",
        "base"
    ],
    "homepage": "https://github.com/ByteNet-Serbia/laravel-admin-base",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikola Zeravcic",
            "email": "office@bytenet.rs",
            "homepage": "http://www.bytenet.rs",
            "role": "CEO, Chief Architect & Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.3",
        "php" : "~5.5|~7.0",
        "laravel/framework": "5.3.*",
        "doctrine/dbal": "^2.5",
        "almasaeed2010/AdminLTE": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*"
    },
    "autoload": {
        "psr-4": {
            "ByteNet\\LaravelAdminBase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ByteNet\\LaravelAdminBase\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
